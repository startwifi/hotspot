%h2= t('.company', name: @company.name)

.col-md-6
  %h3= t('.admins')
  %table.table.table-striped
    %thead
      %th= t('.email')
      %th= t('.last_signin')
    %tbody
      - @admins.each do |admin|
        %tr
          %td= admin.email
          %td= admin.last_sign_in_at
  %p= link_to t('.add_new'), new_admin_company_path(@company), class: 'btn btn-primary'
.col-md-6
  %h3= t('.last_events')

